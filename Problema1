#define MAX_LUNA 12
#define MAX_ANUL 2025

struct Cartof
{
    char date[12];
    unsigned long long int id;
    char *product_name;
    char *product_category;
    char *product_subcategory;
    float unit_price;
    const unsigned int quantity_sold;
    char *sale_country;
    char *sale_city;
    struct Cartof *next;
};
typedef struct Cartof Unit;

// typedef struct an_luna{
