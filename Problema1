}

void free_list(Unit *head)
{
    Unit *temp_head = head;
    while (temp_head != NULL)
    {
        Unit *temp = temp_head;
        temp_head = temp_head->next;
        free(temp->product_name);
        free(temp->product_category);
        free(temp->product_subcategory);
        free(temp->sale_country);
        free(temp->sale_city);
        free(temp);
    }
}

